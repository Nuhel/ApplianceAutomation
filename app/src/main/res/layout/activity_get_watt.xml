<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="nuhel.applianceautomation.GetWatt">

    <TextView
        android:layout_width="120dp"
        android:layout_height="50sp"
        android:layout_above="@+id/textView18"
        android:layout_alignLeft="@+id/textView18"
        android:layout_alignStart="@+id/textView18"
        android:layout_marginBottom="21dp"
        android:text="Channel 1 :"
        android:textColor="#000000"
        android:textSize="20dp"
        android:id="@+id/textView19" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="120dp"
        android:layout_height="50sp"
        android:layout_above="@+id/textView17"
        android:layout_alignLeft="@+id/textView17"
        android:layout_alignStart="@+id/textView17"
        android:layout_marginBottom="20dp"
        android:text="Channel 2 :"
        android:textColor="#000000"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="120dp"
        android:layout_height="50sp"
        android:layout_above="@+id/textView16"
        android:layout_alignLeft="@+id/textView16"
        android:layout_alignStart="@+id/textView16"
        android:layout_marginBottom="15dp"
        android:text="Channel 3 :"
        android:textColor="#000000"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="120dp"
        android:layout_height="50sp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="187dp"
        android:layout_marginLeft="65dp"
        android:layout_marginStart="65dp"
        android:text="Channel 4 :"
        android:textColor="#000000"
        android:textSize="20dp" />

    <TextView
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView19"
        android:layout_marginBottom="20dp"
        android:layout_marginLeft="45dp"
        android:layout_marginStart="45dp"
        android:layout_toEndOf="@+id/textView19"
        android:layout_toRightOf="@+id/textView19"
        android:text="Watt"
        android:textColor="#000000"
        android:textSize="17sp"
        android:id="@+id/textView20" />

    <EditText
        android:id="@+id/one"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignBottom="@+id/textView19"
        android:layout_alignLeft="@+id/textView20"
        android:layout_alignStart="@+id/textView20" />

    <EditText
        android:id="@+id/two"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignBottom="@+id/textView18"
        android:layout_alignLeft="@+id/three"
        android:layout_alignStart="@+id/three" />

    <EditText
        android:id="@+id/three"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignLeft="@+id/four"
        android:layout_alignStart="@+id/four"
        android:layout_alignTop="@+id/textView17" />

    <EditText
        android:id="@+id/four"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignBottom="@+id/textView16"
        android:layout_alignLeft="@+id/textView20"
        android:layout_alignStart="@+id/textView20" />

    <Button
        android:id="@+id/next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/four"
        android:layout_marginTop="53dp"
        android:layout_toLeftOf="@+id/four"
        android:layout_toStartOf="@+id/four"
        android:text="Next" />

</RelativeLayout>
